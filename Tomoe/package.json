{
  "name": "Tomoe",
  "version": "2.0.0",
  "description": "Centralized API user platform for Hackathons",
  "main": "production.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "node ./src/app/build/scripts/compile",
    "setup": "npm run generate",
    "generate": "node ./build/core",
    "build": "npm run clean && cross-env NODE_ENV=production npm run compile",
    "start": "node ./build/imports && cross-env NODE_ENV=development node ./src/app/build/scripts/start",
    "api": "node ./src/server/index.js",
    "test_server": "cross-env NODE_ENV=test node ./build/tests",
    "test_app": "cross-env NODE_ENV=test karma start ./src/app/build/karma.config",
    "test": "npm run test_server",
    "test:watch": "npm test -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HackMerced/Tomoe.git"
  },
  "author": "Shubham Naik <shub@hackmerced.com>",
  "license": "Apache",
